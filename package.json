{
  "name": "identity-vault-mobile",
  "version": "1.0.0",
  "description": "Secure identity verification and blockchain transaction signing with hardware-backed Keycard and NFC authentication",
  "main": "index.js",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint src/**/*.{js,tsx}",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "xcodebuild -workspace ios/HumanPassportMobile.xcworkspace -scheme HumanPassportMobile -configuration Release"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.74.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "react-native-nfc-manager": "^3.14.12",
    "ethers": "^6.10.0",
    "axios": "^1.6.5",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "react-native-keychain": "^8.1.2",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@react-native/babel-preset": "0.74.0",
    "@react-native/eslint-config": "0.74.0",
    "@types/react": "^18.2.43",
    "@types/react-native": "^0.73.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
